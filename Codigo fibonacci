def fibonacci(n):
    fib = [0, 1]
    for i in range(2, n + 1):
        next_fib = fib[i - 1] + fib[i - 2]
        fib.append(next_fib)
    return fib[n]

n = 15  # Cambia este valor para obtener el término deseado
resultado = fibonacci(n)
print(f"El término {n}-ésimo de Fibonacci es {resultado}")
